{
    parserOptions: {
        ecmaVersion: 6,
        sourceType: "module"
    },
    env: {
      node: true,
      es6: true
    },
    extends: "eslint:recommended",

    rules: {
      prefer-const: 2,
      max-len: ["error", {
        code: 120,
        ignoreComments: true,
      }],
      comma-spacing: [ "error", {
       before: false,
       after: true
      }],

    }
}
